{
    "name": "zohocrm/php-sdk",
    "description": "Fork of Zoho CRM API SDK for PHP",
    "type": "library",
    "homepage": "https://github.com/jobs-at/zcrm-php-sdk",
    "authors": [
        {
            "name": "Zoho CRM API Team",
            "email":"support@zohocrm.com",
            "homepage": "https://www.zoho.com/crm/help/customer-support.html",
            "role": "Creator"
        },
        {
            "name": "jobs.at Dev Team",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.7"
    },
    "autoload": {
        "psr-4": {
            "": "src"
        },
        "files": ["src/index.php"]
    }
}
